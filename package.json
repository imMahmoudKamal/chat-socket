{
  "name": "chat-socket",
  "version": "1.0.0",
  "description": "Chat App using socket io",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run start\" \"npm run scss\"",
    "start": "nodemon server.js -e js,scss,html,scc",
    "scss": "sass --watch public/scss/main.scss public/css/main.css",
    "watch-server": "nodemon --watch 'src/**/*' -e ts,tsx --exec 'ts-node' ./src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imMahmoudKamal/chat-socket.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/imMahmoudKamal/chat-socket/issues"
  },
  "homepage": "https://github.com/imMahmoudKamal/chat-socket#readme",
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "socket.io": "^4.4.1"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "sass": "^1.49.9"
  },
  "type": "module"
}