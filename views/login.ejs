<form class="form" method="POST" action="./chat" autocomplete="off">
  <h1 class="form__heading">Join Chat</h1>

  <fieldset class="form__group">
    <input class="form__group__field" type="text" name="name" id="name" required data-felid />
    <label class="form__group__label" for="name">Name</label>
  </fieldset>

  <fieldset class="form__group">
    <select class="form__group__field form__group__field--full" name="room" id="room" required>
      <% ROOMS.forEach(room => { %>
      <option value="<%= room %>"><%= room %></option>
      <% }) %>
    </select>
    <label class="form__group__label" for="room">Room</label>
  </fieldset>

  <button class="form__submit" type="submit">Join Room</button>
</form>
